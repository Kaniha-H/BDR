    {{ form_help(form.description) }}
</div>

{# Univers du jeu #}
<div class="form-group">
    {{ form_label(form.universe) }}
    {{ form_widget(form.universe) }}
    {{ form_errors(form.universe) }}
    {{ form_help(form.universe) }}
</div>
